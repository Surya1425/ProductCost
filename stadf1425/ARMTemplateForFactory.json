{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "stadf1425"
		},
		"JsonLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'JsonLS'"
		},
		"csvLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'csvLS'"
		},
		"JsonLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ststorageacc1.dfs.core.windows.net/"
		},
		"csvLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ststorageacc1.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/Practice1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "CleansedCSV",
						"type": "ExecuteDataFlow",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"dataflow": {
								"referenceName": "CleaningCSV",
								"type": "DataFlowReference",
								"parameters": {},
								"datasetParameters": {
									"inputCSV": {},
									"DataScientist": {}
								}
							},
							"staging": {},
							"compute": {
								"coreCount": 8,
								"computeType": "General"
							},
							"traceLevel": "Fine"
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": [],
				"lastPublishTime": "2022-09-15T17:59:42Z"
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/dataflows/CleaningCSV')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/JSONOP')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "JsonLS",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileSystem": "scontainer"
					}
				},
				"schema": {}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/JsonLS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/csvFile')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "csvLS",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileName": "ds_salaries.csv",
						"fileSystem": "scontainer"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "Id",
						"type": "String"
					},
					{
						"name": "work_year",
						"type": "String"
					},
					{
						"name": "experience_level",
						"type": "String"
					},
					{
						"name": "employment_type",
						"type": "String"
					},
					{
						"name": "job_title",
						"type": "String"
					},
					{
						"name": "salary",
						"type": "String"
					},
					{
						"name": "salary_currency",
						"type": "String"
					},
					{
						"name": "salary_in_usd",
						"type": "String"
					},
					{
						"name": "employee_residence",
						"type": "String"
					},
					{
						"name": "remote_ratio",
						"type": "String"
					},
					{
						"name": "company_location",
						"type": "String"
					},
					{
						"name": "company_size",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/csvLS')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/JsonLS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('JsonLS_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('JsonLS_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/csvLS')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('csvLS_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('csvLS_accountKey')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/CleaningCSV')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "csvFile",
								"type": "DatasetReference"
							},
							"name": "inputCSV"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "JSONOP",
								"type": "DatasetReference"
							},
							"name": "DataScientist"
						}
					],
					"transformations": [
						{
							"name": "RemovedNulls"
						},
						{
							"name": "ReplacingNullsWith"
						},
						{
							"name": "filterOnlyDSandDA"
						},
						{
							"name": "SumOfSalaries"
						}
					],
					"scriptLines": [
						"source(output(",
						"          Id as short,",
						"          work_year as short,",
						"          experience_level as string,",
						"          employment_type as string,",
						"          job_title as string,",
						"          salary as integer,",
						"          salary_currency as string,",
						"          salary_in_usd as integer,",
						"          employee_residence as string,",
						"          remote_ratio as short,",
						"          company_location as string,",
						"          company_size as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> inputCSV",
						"ReplacingNullsWith split(isNull(Id),",
						"     disjoint: false) ~> RemovedNulls@(NullRows, GoodRows)",
						"inputCSV derive(work_year = iif(work_year==2020,2022,2023)) ~> ReplacingNullsWith",
						"RemovedNulls@GoodRows filter((job_title==\"Data Scientist\") || (job_title==\"Data Analyst\")) ~> filterOnlyDSandDA",
						"filterOnlyDSandDA aggregate(groupBy(job_title),",
						"     salary = sum(salary)) ~> SumOfSalaries",
						"SumOfSalaries sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     partitionFileNames:['OutputJson'],",
						"     umask: 0022,",
						"     preCommands: [],",
						"     postCommands: [],",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     partitionBy('hash', 1)) ~> DataScientist"
					]
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/csvFile')]",
				"[concat(variables('factoryId'), '/datasets/JSONOP')]"
			]
		}
	]
}